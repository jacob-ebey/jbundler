{
  "name": "jbundler",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "jbundler": "bin/cli.cjs"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./client-entry": {
      "types": "./virtual/client-entry.d.ts",
      "default": "./virtual/client-entry.js"
    },
    "./build-target": {
      "types": "./virtual/build-target.d.ts",
      "default": "./virtual/build-target.js"
    },
    "./rsc-browser-runtime": "./rsc/rsc-browser-runtime.js",
    "./rsc-context": "./rsc/rsc-context.js",
    "./rsc-server-runtime": "./rsc/rsc-server-runtime.js",
    "./webpack-map": {
      "types": "./virtual/webpack-map.d.ts",
      "default": "./virtual/webpack-map.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@babel/core": "7.21.0",
    "arg": "5.0.2",
    "esbuild": "0.17.10",
    "glob-to-regexp": "0.4.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/babel__core": "7.20.0",
    "@types/glob-to-regexp": "0.4.1"
  }
}
